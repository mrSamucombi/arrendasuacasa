{
  "name": "arrendasuacasa-server",
  "version": "1.0.0",
  "description": "Backend for ArrendaSuaCasa",
  "main": "dist/server ",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only server ",
